<package>
	<id>apktw.com.rpetrich.biolockdown</id>
	<name>BioLockdown</name>
  <name>（已破解DRM）</name>
	<version>1.3.1</version>
	<compatibility>
		<firmware>
			<miniOS>6.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
    <package>firmware (>= 6.0)</package>
		<package>mobilesubstrate</package>
		<package>preferenceloader</package>
    <package>applist</package>
		<package>com.a3tweaks.flipswitch</package>
		<package>com.rpetrich.rocketbootstrap</package>
	</dependencies>
	<descriptionlist>
		<description>Fingerprint protect access to apps, settings and Control Center switches</description>
		<description>Designed for the Touch ID sensor with additional support for passcodes</description>
	</descriptionlist>
